							</columns>
						</TableView>
					</content>
				</Tab>
				<Tab text=" 已审核 ">
					<content>

						<TableView fx:id="validTable" editable="true" prefHeight="200.0" prefWidth="200.0">
							<columns>
								<TableColumn prefWidth="311.0" text="账户地址" />
								<TableColumn minWidth="0.0" prefWidth="100.0" text="姓名" />
								<TableColumn prefWidth="276.0" text="ID" />
							</columns>
						</TableView>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</children>
</AnchorPane>
