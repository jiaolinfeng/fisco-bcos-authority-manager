			</items>
		</ToolBar>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0">
			<tabs>
				<Tab text=" 待审核 ">
					<content>

						<TableView fx:id="regTable" editable="true" prefHeight="200.0" prefWidth="200.0">
							<columns>
								<TableColumn prefWidth="289.0" text="账户地址" />
								<TableColumn minWidth="0.0" prefWidth="122.0" text="姓名" />
								<TableColumn prefWidth="225.0" text="ID" />
