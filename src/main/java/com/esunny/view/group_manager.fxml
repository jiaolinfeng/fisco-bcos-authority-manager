<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" minHeight="0.0" minWidth="0.0" prefHeight="446.0" prefWidth="732.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esunny.controller.GroupManagerController">
	<children>
		<ToolBar prefHeight="50.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<items>
				<Button mnemonicParsing="false" onAction="#addGroup" text="创建组" />
				<Button mnemonicParsing="false" onAction="#importGroup" text="导入组" />
			</items>
		</ToolBar>
      <TabPane fx:id="groupPane" prefHeight="296.0" prefWidth="584.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0" />
      <ToolBar layoutX="26.0" layoutY="326.0" prefHeight="50.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <items>
            <Label text="CNS工具" />
            <TextField prefHeight="25.0" prefWidth="200.0" promptText="合约名称" fx:id="contractNameField" />
            <TextField editable="false" prefHeight="25.0" prefWidth="320.0"  fx:id="contractAddrField" promptText="合约地址" />
